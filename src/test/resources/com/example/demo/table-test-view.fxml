<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" prefHeight="400.0" prefWidth="300.0" spacing="10.0" stylesheets="@style.css" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.demo.controller.TableTestController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>

    <Button fx:id="back" onAction="#goBack" styleClass="button-2" text="Go back" />
    <TableView fx:id="tableResult" layoutX="12.0" layoutY="62.0" prefHeight="325.0" prefWidth="145.0">
        <columns>
            <!-- Колонки таблицы -->
            <TableColumn fx:id="idColumn" prefWidth="31" text="№" />
            <TableColumn fx:id="nameColumn" prefWidth="128" text="Name" />
            <TableColumn fx:id="resultColumn" prefWidth="98" text="Result" />
        </columns>
    </TableView>
</VBox>
