<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<VBox alignment="CENTER" spacing="10.0" xmlns=" http://javafx.com/javafx"
      xmlns:fx="http://javafx.com/fxml"
      fx:controller="com.example.demo.controller.old.HelloController" stylesheets="@../style.css">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0"/>
    </padding>

    <Button fx:id="selectButton" text="Select One File and Open" onAction="#runTestsFromJarFile"
            styleClass="button-2"/>
    <Button fx:id="back" text="Go back" onAction="#goBack" styleClass="button-2"/>
    <HBox alignment="CENTER" spacing="10.0">
        <Button fx:id="infoButton" text="Tab to get more info" onAction="#getInfoAboutTests"
                visible="false" styleClass="button-2"/>
        <Button fx:id="attempt" text="Your attempts" onAction="#getInfoAboutAttempts"
                visible="false" styleClass="button-2"/>
    </HBox>

    <TextArea fx:id="textArea" visible="false"/>
</VBox>
