<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.VBox?>
<VBox alignment="CENTER" spacing="10.0" xmlns=" http://javafx.com/javafx"
      xmlns:fx="http://javafx.com/fxml"
      fx:controller="com.example.demo.controller.TableAttemptController" stylesheets="@style.css">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0"/>
    </padding>

    <Button fx:id="back" text="Go back" onAction="#goBack" styleClass="button-2"/>
    <TableView fx:id="tableAttempts" layoutX="12.0" layoutY="62.0" prefHeight="325.0" prefWidth="145.0">
        <columns>
            <!-- Колонки таблицы -->
            <TableColumn prefWidth="31" text="№" fx:id="idColumn" />
            <TableColumn prefWidth="113" text="Passed" fx:id="passedColumn" />
            <TableColumn prefWidth="113" text="Failed" fx:id="failedColumn" />
        </columns>
    </TableView>
</VBox>
